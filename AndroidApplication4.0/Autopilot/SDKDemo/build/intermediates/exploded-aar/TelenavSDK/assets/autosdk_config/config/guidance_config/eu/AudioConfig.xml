<?xml version="1.0" encoding="utf-8"?>

<!-- version -->
<Version value="2.0" />

<!-- spec(current only support "ARP" & "GEN3") -->
<AudioSpec value="ARP" />

<!-- whether Prompts (Wolverine) or TTS string (Logan)
     Prompts or TTS -->
<GuidanceType value="TTS" />

<!-- Whether name uses phoneme or orthography -->
<AddressNameType value="orthographyOnly" />


<Phoneme>
	<EscapePrefix prefix="\x1b\\lang=%s\\" />
    <EscapeSuffix suffix="\x1b\\toi=orth\\" />

    <!-- phoneme tag format, '[LNG=NT-%s]' or '[LNG=%s]' or empty -->
    <PhonemeTagFormat format="\x1b\\toi=nts\\" />

    <!-- slot prefix and postfix for wolverine, can be <TTS<slot>> or <TTS<slot2>>, '2' is the index of tts slot -->
    <SlotGeneralFormat format="<TTS<slot%d>>" />
    <SlotStreetFormat format="<STREETTTS<slot%d>>" />
    <SlotCityFormat format="<CITYTTS<slot%d>>" />
	
    <!---in our map data, the phoeme tag may match our TTS engine -->
    <PhonemeTagMapping>
        <ENU dataTag="ENG" />
        <FRC dataTag="FRE;CFR" />
        <SPU dataTag="NAS;SPM;SPA" />
    </PhonemeTagMapping>

    <Language_TTS_Supported>
