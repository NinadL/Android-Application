<Switches>
  <!-- Logan need repeat 3rd Guidance, but Wolverine need repeat 2nd true of false -->
  <Repeat3rdGuidance value="true" />
  <PlayObeyLawGuidance value="true" />
  <DistinguishRoundaboutType value="true" />
  <EnableSignBNameAsNextStName value="true" />
  <UseSingleContinueTemplate value="true" />
</Switches>

