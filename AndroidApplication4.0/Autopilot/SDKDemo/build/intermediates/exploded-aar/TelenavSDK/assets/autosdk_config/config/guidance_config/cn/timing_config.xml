<!-- minimum time gap between two back-to-back guidances, unit is millisecond. -->
<GuidanceInterval value="1000" />

<GuidanceConstant>
	<ConstantSet unit="Imperial">
		<Dist length="197" type="info_advance" unit="feet" />
